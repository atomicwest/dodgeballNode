<form method="POST" action="/dodgeGame" id="formgroup">
    
  <div class="form-group">
    <input type="text" id="scorename" name="name" placeholder="Enter Your Name"></input>
  </div>
  
  <div class="form-group">
    <input type="text" id="scorefield" name="score" value=<%=score%> readonly="readonly">
    </input>
  </div>

  <button type="submit" class="btn btn-secondary" id="sendscore">Add to Hall of Fame</button>
</form>